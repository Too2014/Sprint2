<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="postgres" match-schema="public"/>
  <table-filter match-name="usuario"/>
  <table-filter match-name="archivos"/>
  <table-filter match-name="pregunta"/>
  <table-filter match-name="encuesta"/>
  <table-filter match-name="respuesta"/>
  <table-filter match-name="categoria"/>
</hibernate-reverse-engineering>
